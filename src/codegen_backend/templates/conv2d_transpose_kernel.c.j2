{{ signature }}
    ssize_t in_per_group = {{ in_channels }} / {{ groups }};
    ssize_t out_per_group = {{ out_channels }} / {{ groups }};
    ssize_t out_pad_h = {{ out_pad_h }};
    ssize_t out_pad_w = {{ out_pad_w }};
    (void)out_pad_h;
    (void)out_pad_w;
    {% if has_batch %}
    for (ssize_t n = 0; n < {{ batch }}; ++n) {
        for (ssize_t oc = 0; oc < {{ out_channels }}; ++oc) {
            for (ssize_t oh = 0; oh < {{ out_h }}; ++oh) {
                for (ssize_t ow = 0; ow < {{ out_w }}; ++ow) {
                    {% if has_bias %}
                    out[n][oc][oh][ow] = bias[oc];
                    {% else %}
                    out[n][oc][oh][ow] = 0.0f;
                    {% endif %}
                }
            }
        }
        for (ssize_t ic = 0; ic < {{ in_channels }}; ++ic) {
            ssize_t group = (ssize_t)ic / in_per_group;
            for (ssize_t ih = 0; ih < {{ in_h }}; ++ih) {
                for (ssize_t iw = 0; iw < {{ in_w }}; ++iw) {
                    {{ c_type }} value = {{ input_access }};
                    for (ssize_t kh = 0; kh < {{ k_h }}; ++kh) {
                        ssize_t oh = (ssize_t)ih * {{ stride_h }} - {{ pad_h }} + (ssize_t)kh * {{ dil_h }};
                        if (oh < 0 || oh >= {{ out_h }}) {
                            continue;
                        }
                        for (ssize_t kw = 0; kw < {{ k_w }}; ++kw) {
                            ssize_t ow = (ssize_t)iw * {{ stride_w }} - {{ pad_w }} + (ssize_t)kw * {{ dil_w }};
                            if (ow < 0 || ow >= {{ out_w }}) {
                                continue;
                            }
                            for (ssize_t oc = 0; oc < out_per_group; ++oc) {
                                ssize_t out_c = group * out_per_group + (ssize_t)oc;
                                out[n][out_c][oh][ow] +=
                                    value * {{ weight_access }};
                            }
                        }
                    }
                }
            }
        }
    }
    {% else %}
    for (ssize_t oc = 0; oc < {{ out_channels }}; ++oc) {
        for (ssize_t oh = 0; oh < {{ out_h }}; ++oh) {
            for (ssize_t ow = 0; ow < {{ out_w }}; ++ow) {
                {% if has_bias %}
                out[oc][oh][ow] = bias[oc];
                {% else %}
                out[oc][oh][ow] = 0.0f;
                {% endif %}
            }
        }
    }
    for (ssize_t ic = 0; ic < {{ in_channels }}; ++ic) {
        ssize_t group = (ssize_t)ic / in_per_group;
        for (ssize_t ih = 0; ih < {{ in_h }}; ++ih) {
            for (ssize_t iw = 0; iw < {{ in_w }}; ++iw) {
                {{ c_type }} value = {{ input_access }};
                for (ssize_t kh = 0; kh < {{ k_h }}; ++kh) {
                    ssize_t oh = (ssize_t)ih * {{ stride_h }} - {{ pad_h }} + (ssize_t)kh * {{ dil_h }};
                    if (oh < 0 || oh >= {{ out_h }}) {
                        continue;
                    }
                    for (ssize_t kw = 0; kw < {{ k_w }}; ++kw) {
                        ssize_t ow = (ssize_t)iw * {{ stride_w }} - {{ pad_w }} + (ssize_t)kw * {{ dil_w }};
                        if (ow < 0 || ow >= {{ out_w }}) {
                            continue;
                        }
                        for (ssize_t oc = 0; oc < out_per_group; ++oc) {
                            ssize_t out_c = group * out_per_group + (ssize_t)oc;
                            out[out_c][oh][ow] +=
                                value * {{ weight_access }};
                        }
                    }
                }
            }
        }
    }
    {% endif %}
}
