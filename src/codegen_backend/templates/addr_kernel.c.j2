{{ signature }}
    for (size_t i = 0; i < {{ m }}; ++i) {
        for (size_t j = 0; j < {{ n }}; ++j) {
            {%- if skip_input %}
            {{ out_access }} = ({{ alpha }}) * {{ vec1_access }} * {{ vec2_access }};
            {%- else %}
            {{ out_access }} = ({{ beta }}) * {{ input_access }} + ({{ alpha }}) * {{ vec1_access }} * {{ vec2_access }};
            {%- endif %}
        }
    }
}
