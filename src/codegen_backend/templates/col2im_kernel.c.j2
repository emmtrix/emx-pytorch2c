{{ signature }}
    {% if has_batch %}
    for (size_t n = 0; n < {{ batch }}; ++n) {
        for (size_t c = 0; c < {{ channels }}; ++c) {
            for (size_t h = 0; h < {{ out_h }}; ++h) {
                for (size_t w = 0; w < {{ out_w }}; ++w) {
                    out[n][c][h][w] = ({{ c_type }})0;
                }
            }
        }
    }
    for (size_t n = 0; n < {{ batch }}; ++n) {
        for (size_t c = 0; c < {{ channels }}; ++c) {
            for (size_t kh = 0; kh < {{ k_h }}; ++kh) {
                for (size_t kw = 0; kw < {{ k_w }}; ++kw) {
                    int64_t col_c = ((int64_t)c * {{ k_h }} + (int64_t)kh) * {{ k_w }} + (int64_t)kw;
                    for (size_t oh = 0; oh < {{ out_blocks_h }}; ++oh) {
                        int64_t out_h_idx = (int64_t)oh * {{ stride_h }} - {{ pad_h }} + (int64_t)kh * {{ dil_h }};
                        if (out_h_idx < 0 || out_h_idx >= {{ out_h }}) {
                            continue;
                        }
                        for (size_t ow = 0; ow < {{ out_blocks_w }}; ++ow) {
                            int64_t out_w_idx = (int64_t)ow * {{ stride_w }} - {{ pad_w }} + (int64_t)kw * {{ dil_w }};
                            if (out_w_idx < 0 || out_w_idx >= {{ out_w }}) {
                                continue;
                            }
                            int64_t col_l = (int64_t)oh * {{ out_blocks_w }} + (int64_t)ow;
                            out[n][c][out_h_idx][out_w_idx] += input[n][col_c][col_l];
                        }
                    }
                }
            }
        }
    }
    {% else %}
    for (size_t c = 0; c < {{ channels }}; ++c) {
        for (size_t h = 0; h < {{ out_h }}; ++h) {
            for (size_t w = 0; w < {{ out_w }}; ++w) {
                out[c][h][w] = ({{ c_type }})0;
            }
        }
    }
    for (size_t c = 0; c < {{ channels }}; ++c) {
        for (size_t kh = 0; kh < {{ k_h }}; ++kh) {
            for (size_t kw = 0; kw < {{ k_w }}; ++kw) {
                int64_t col_c = ((int64_t)c * {{ k_h }} + (int64_t)kh) * {{ k_w }} + (int64_t)kw;
                for (size_t oh = 0; oh < {{ out_blocks_h }}; ++oh) {
                    int64_t out_h_idx = (int64_t)oh * {{ stride_h }} - {{ pad_h }} + (int64_t)kh * {{ dil_h }};
                    if (out_h_idx < 0 || out_h_idx >= {{ out_h }}) {
                        continue;
                    }
                    for (size_t ow = 0; ow < {{ out_blocks_w }}; ++ow) {
                        int64_t out_w_idx = (int64_t)ow * {{ stride_w }} - {{ pad_w }} + (int64_t)kw * {{ dil_w }};
                        if (out_w_idx < 0 || out_w_idx >= {{ out_w }}) {
                            continue;
                        }
                        int64_t col_l = (int64_t)oh * {{ out_blocks_w }} + (int64_t)ow;
                        out[c][out_h_idx][out_w_idx] += input[col_c][col_l];
                    }
                }
            }
        }
    }
    {% endif %}
}
