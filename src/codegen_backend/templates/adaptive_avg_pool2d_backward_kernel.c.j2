{{ signature }}
    for (ssize_t n = 0; n < {{ batch }}; ++n) {
        for (ssize_t c = 0; c < {{ channels }}; ++c) {
            for (ssize_t oh = 0; oh < {{ out_h }}; ++oh) {
                for (ssize_t ow = 0; ow < {{ out_w }}; ++ow) {
                    {{ c_type }} grad = grad_output[n][c][oh][ow] / ({{ c_type }})({{ k_h }} * {{ k_w }});
                    ssize_t in_h_base = (ssize_t)oh * {{ stride_h }};
                    ssize_t in_w_base = (ssize_t)ow * {{ stride_w }};
                    for (ssize_t kh = 0; kh < {{ k_h }}; ++kh) {
                        ssize_t in_h_idx = in_h_base + (ssize_t)kh;
                        for (ssize_t kw = 0; kw < {{ k_w }}; ++kw) {
                            ssize_t in_w_idx = in_w_base + (ssize_t)kw;
                            out[n][c][in_h_idx][in_w_idx] = grad;
                        }
                    }
                }
            }
        }
    }
}
